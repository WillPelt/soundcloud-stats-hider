{

    "manifest_version": 3,
    "name": "Soundcloud-Stats-Hider",
    "description": "Hides soundcloud stats.",
    "version": "1.0",
    "icons": {
      "48": "images/play.png"
    },
  
    "content_scripts": [
      {
        "run_at": "document_end",
        "js": ["scripts/soundcloud-stats-hider.js"],
        "matches": ["*://*.soundcloud.com/*"]
      }
    ]
  
  }
  