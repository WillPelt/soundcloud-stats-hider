{

    "manifest_version": 2,
    "name": "Soundcloud-Stats-Hider",
    "version": "2.0",
  
    "description": "Hides soundcloud stats.",
  
    "icons": {
      "48": "icons/play.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.soundcloud.com/*"],
        "js": ["soundcloud-stats-hider.js"]
      }
    ]
  
  }
  